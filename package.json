{
	"name": "lift_trckr",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 9001",
		"prod": "vite dev --port 9001 --mode production",
		"prepare": "svelte-kit sync",
		"build": "vite build",
		"build:ci": "vite build --mode ci",
		"docker": "docker compose up -d",
		"preview": "vite preview --port 9001",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier": "prettier --plugin-search-dir . --check .",
		"lint": "eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:introspect": "drizzle-kit introspect:pg",
		"db:generate": "drizzle-kit generate:pg",
		"db:generate-custom": "drizzle-kit generate:pg --custom",
		"db:migrate": "tsx -r dotenv/config ./src/db/migrate.ts",
		"db:studio": "npx drizzle-kit studio"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-vercel": "^5.2.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/debounce": "^1.2.4",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pg": "^8.11.3",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.64.2",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^3.12.7",
		"typescript": "^5.4.3",
		"vite": "^5.2.14"
	},
	"dependencies": {
		"@auth/core": "^0.12.0",
		"@auth/drizzle-adapter": "^0.3.2",
		"@auth/sveltekit": "^0.3.6",
		"@neondatabase/serverless": "^0.6.0",
		"axios": "^1.6.0",
		"bits-ui": "^0.21.5",
		"debounce": "^1.2.1",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.6",
		"drizzle-zod": "^0.5.1",
		"moment": "^2.29.4",
		"normalize.css": "^8.0.1",
		"postgres": "^3.3.5",
		"svelte-french-toast": "^1.2.0",
		"ws": "^8.16.0",
		"zod": "^3.22.2"
	}
}
