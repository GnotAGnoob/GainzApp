{
	"name": "LiftTrckr",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 9001",
		"master": "vite dev --port 9001 --mode master",
		"prod": "vite dev --port 9001 --mode production",
		"build": "vite build",
		"build:master": "vite build",
		"docker": "docker compose up -d",
		"preview": "vite preview --port 9001",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"introspect": "drizzle-kit introspect:pg",
		"generate": "drizzle-kit generate:pg",
		"migrate": "tsx -r dotenv/config ./src/db/migrate.ts"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pg": "^8.11.3",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.64.2",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^3.12.7",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@neondatabase/serverless": "^0.6.0",
		"@svelteuidev/composables": "^0.15.0",
		"@svelteuidev/core": "^0.15.0",
		"@svelteuidev/dates": "^0.15.0",
		"dayjs": "^1.11.9",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.5",
		"normalize.css": "^8.0.1",
		"postgres": "^3.3.5",
		"svelte-french-toast": "^1.2.0"
	}
}
